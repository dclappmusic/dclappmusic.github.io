    "core-js": "^3.8.0",
    "firebase": "^8.6.2",
    "mapbox-gl": "^1.13.0",
    "moment": "^2.29.1",
    "register-service-worker": "^1.7.1",
    "vue": "^2.6.12",
    "vue-i18n": "^8.22.2",
    "vue-mapbox": "^0.4.1",
    "vue-mapbox-geocoder": "^0.2.0",
    "vue-router": "^3.4.9",
    "vue2-touch-events": "^2.3.2",
    "vuex": "^3.6.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-pwa": "^4.2.0",
    "@vue/cli-service": "^4.5.9",
    "babel-eslint": "^10.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
